{
  "manifest_version": 3,
  "name": "js-injecting-extension",
  "version": "0.1",
  "description": "Execute custom JavaScript code on user-defined URLs",
  "permissions": ["activeTab", "tabs", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
