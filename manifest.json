{
  "manifest_version": 3,
  "name": "js-injecting-extension",
  "version": "0.1",
  "description": "Execute custom JavaScript code on user-defined URLs",
  "permissions": ["activeTab", "storage", "scripting", "webNavigation"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
